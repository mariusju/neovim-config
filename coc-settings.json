{
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": false,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.checkCurrentLine": true,

  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.errorSign": "->",
  "diagnostic.infoSign": "->",
  "diagnostic.warningSign": "->",
  "diagnostic.hintSign": "->",

  "diagnostic.format": "%message",

  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typesccriptreact"
  ],
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5][size]",
  "explorer.previewAction.onHover": false,
  "explorer.file.root.template": "[fullpath]",
  "explorer.width": 50
}
